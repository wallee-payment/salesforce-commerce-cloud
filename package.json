  "browserslist": [
    "ie >= 10",
    "last 2 versions"
  ],
  "dependencies": {},
  "description": "New overlay cartridge",
  "devDependencies": {
    "@types/jquery": "^3.3.31",
    "chai": "^3.5.0",
    "css-loader": "^3.2.0",
    "eslint": ">=4.18.2",
    "eslint-plugin-es5": "^1.4.1",
    "eslint-plugin-import": ">=2.19.1",
    "istanbul": "^0.4.4",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "proxyquire": "1.7.4",
    "sass-loader": "^7.0.3",
    "sgmf-scripts": "^2.0.0",
    "sinon": "^1.17.4",
    "stylelint": "^12.0.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.11.1",
    "tslint-lines-between-class-members": "^1.3.6",
    "typescript": "^3.7.4",
    "watch": "^1.0.2"
  },
  "license": "Apache-2.0",
  "main": "index.js",
  "name": "int_wallee",
  "packageName": "int_wallee",
  "repository": {
    "type": "git",
    "url": "http://github.com/wallee-payment/salesforce-commerce-cloud.git"
  },
  "scripts": {
    "build": "npm run build:tsc && npm run build:eslint && npm run build:upload",
    "build:eslint": "./node_modules/.bin/eslint ./cartridges/int_wallee/. --ext .js",
    "build:tsc": "tsc --project ./tsconfig.json",
    "build:upload": "rm -fr ./cartridges/int_wallee/* && cp -rf ./cartridges/int_wallee/* ./cartridges/int_wallee/",
    "compile:js": "sgmf-scripts --compile js",
    "compile:scss": "sgmf-scripts --compile css",
    "lint": "sgmf-scripts --lint js && sgmf-scripts --lint css",
    "upload": "sgmf-scripts --upload -- ",
    "uploadCartridge": "sgmf-scripts --uploadCartridge int_wallee",
    "watch": "watch 'npm run build' ./buildSrc/src/main/resources/cartridges/"
  },
  "version": "1.0.0"
}
